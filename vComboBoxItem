public class vComboBoxItem {
    private String id;
    private String value;

    public vComboBoxItem(String id, String value) {
        this.id = id;
        this.value = value;
    }

    public String getId() {
        return id;
    }

    public String getValue() {
        return value;
    }

    @Override
    public String toString() {
        return value;  // The value will be displayed in the ComboBox
    } 
}
